{
  "agent": {
    "metrics_collection_interval": 60,
    "region": "us-east-1",
    "logfile": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
    "debug": false
  },
  "logs": {
    "logs_collected": {
      "files": {
      "collect_list": [
        {
        "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
        "log_group_name": "environment/service/application//opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
        "log_stream_name": "{hostname}_{instance_id}",
        "timezone": "UTC"
        }
      ]
      }
    },
    "log_stream_name": "environment/service"
  }
}